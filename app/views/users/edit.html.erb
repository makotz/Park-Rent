<h1>Edit Account Info <%= link_to "Forgot Password?", edit_password_path  %></h1>
<%= simple_form_for @user, url:users_path do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
  <%= f.button :submit, class: "btn btn-primary" do %>
    Save changes
  <% end %>
<% end %>
</div>
